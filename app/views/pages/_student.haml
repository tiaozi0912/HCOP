%article
  %section
    .row-fluid
      .span3
        %img.img-circle.shadow{:src=> "#{@student.photo}"}
      .span8#student-name
        %h2 #{@student.name}
        %span #{@student.school}
  %section
    %h4 Profile
    %p #{@student.profile}
  %section
    %h4 Project
    %p #{@student.project}
